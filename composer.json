{
    "name": "nabu-3/provider-apache-httpd",
    "description": "nabu-3 provider to add Apache Server support",
    "version": "0.0.8",
    "type": "nabu-provider",
    "keywords": [ "nabu-3", "nabu", "apache", "http", "https", "httpd", "provider" ],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Rafael Gutierrez",
            "email": "ragutimar@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/nabu-3/provider-apache-httpd/issues"
    },
    "config": {
        "bin-dir": "bin",
        "vendor-dir": "lib"
    },
    "require": {
        "nabu-3/composer-installer-plugin": ">=3.0"
    },
    "require-dev": {
        "nabu-3/composer-installer-plugin": "dev-master"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/nabu-3/provider-apache-httpd"
        }
    ],
    "bin": [
        "bin/nabu-apache-manager.sh"
    ]
}
